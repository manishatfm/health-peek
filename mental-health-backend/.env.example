# Server
PORT=8000

# Google OAuth
GOOGLE_CLIENT_ID=your-google-client-id
GOOGLE_CLIENT_SECRET=your-google-client-secret
GOOGLE_REDIRECT_URI=http://localhost:3000/oauth2/redirect

# JWT
JWT_SECRET=change-this-to-a-strong-random-secret

# Frontend
FRONTEND_URL=http://localhost:3000
FRONTEND_REDIRECT_URI=http://localhost:3000/auth/callback

# MongoDB Atlas (create free cluster at https://cloud.mongodb.com)
DATABASE_URL=mongodb+srv://<username>:<password>@<cluster>.mongodb.net/?retryWrites=true&w=majority
DATABASE_NAME=mental_health_db

# Debug
DEBUG=true
ENABLE_CORS=true
